{
  "manifest_version": 3,
  "name": "LinkedIn Job Saver",
  "version": "1.0",
  "description": "Captures job details from the current LinkedIn job page.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
